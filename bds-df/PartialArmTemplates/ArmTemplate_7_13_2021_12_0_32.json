										"sink": {
											"name": "triggered_time",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "bdsAllData",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
